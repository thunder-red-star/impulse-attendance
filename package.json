{
  "name": "impulse-attendance",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.js",
  "bin": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tap \"test/**/*.test.js\"",
    "start": "node app.js",
    "dev": "nodemon --watch app.js --exec \"npm start\"",
    "prod": "node app.js",
    "init": "node init.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thunder-red-star/impulse-attendance.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thunder-red-star/impulse-attendance/issues"
  },
  "homepage": "https://github.com/thunder-red-star/impulse-attendance#readme",
  "dependencies": {
    "@fastify/autoload": "^4.0.1",
    "@fastify/static": "^5.0.2",
    "fastify": "^3.0.0",
    "fastify-autoload": "^3.10.0",
    "fastify-cli": "^2.15.0",
    "fastify-plugin": "^3.0.0",
    "fastify-sensible": "^3.1.2",
    "fastify-static": "^4.7.0",
    "pkg": "^5.7.0",
    "socket.io": "^4.5.1"
  },
  "devDependencies": {
    "tap": "^15.1.6"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": "public/**/*",
    "targets": [
      "node16-linux-x64",
      "node16-macos-x64",
      "node16-win-x64"
    ],
    "outputPath": "dist"
  }
}
